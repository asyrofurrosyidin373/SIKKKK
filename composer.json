{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/sanctum": "^4.2"
    },
    "require-dev": {
        "anourvalar/eloquent-serialize": "^1.3",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "blade-ui-kit/blade-icons": "^1.8",
        "brick/math": "^0.13",
        "carbonphp/carbon-doctrine-types": "^3.2",
        "danharrin/date-format-converter": "^0.3",
        "danharrin/livewire-rate-limiting": "^2.1",
        "dflydev/dot-access-data": "^3.0",
        "doctrine/dbal": "^4.3",
        "doctrine/deprecations": "^1.1",
        "doctrine/inflector": "^2.1",
        "doctrine/lexer": "^3.0",
        "dragonmantank/cron-expression": "^3.4",
        "egulias/email-validator": "^4.0",
        "fakerphp/faker": "^1.24",
        "filament/actions": "^3.3",
        "filament/filament": "^3.3",
        "filament/forms": "^3.3",
        "filament/infolists": "^3.3",
        "filament/notifications": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "filament/spatie-laravel-settings-plugin": "^3.3",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "filament/support": "^3.3",
        "filament/tables": "^3.3",
        "filament/widgets": "^3.3",
        "filp/whoops": "^2.18",
        "fruitcake/php-cors": "^1.3",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/gif": "^4.2",
        "intervention/image": "^3.11",
        "kirschbaum-development/eloquent-power-joins": "^4.2",
        "laravel/framework": "^12.25",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.24",
        "laravel/prompts": "^0.3",
        "laravel/sail": "^1.44",
        "laravel/serializable-closure": "^2.0",
        "laravel/tinker": "^2.10",
        "league/commonmark": "^2.7",
        "league/config": "^1.2",
        "league/csv": "^9.24",
        "league/flysystem": "^3.30",
        "league/flysystem-local": "^3.30",
        "league/mime-type-detection": "^1.16",
        "league/uri": "^7.5",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^1.1",
        "maennchen/zipstream-php": "^3.2",
        "masterminds/html5": "^2.10",
        "mockery/mockery": "^1.6",
        "monolog/monolog": "^3.9",
        "nesbot/carbon": "^3.10",
        "nette/schema": "^1.3",
        "nette/utils": "^4.0",
        "nikic/php-parser": "^5.6",
        "nunomaduro/collision": "^8.8",
        "nunomaduro/termwind": "^2.3",
        "openspout/openspout": "^4.30",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^5.0",
        "phpunit/phpunit": "^11.5",
        "psr/container": "^2.0",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.9",
        "ryangjchandler/blade-capture-directive": "^1.1",
        "spatie/color": "^1.8",
        "spatie/eloquent-sortable": "^4.5",
        "spatie/image": "^3.8",
        "spatie/image-optimizer": "^1.8",
        "spatie/invade": "^2.1",
        "spatie/laravel-medialibrary": "^11.14",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-tags": "^4.10",
        "spatie/laravel-translatable": "^6.11",
        "symfony/console": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/error-handler": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/finder": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/mime": "^7.3",
        "symfony/process": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/string": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/uid": "^7.3",
        "symfony/var-dumper": "^7.3",
        "symfony/yaml": "^7.3",
        "tijsverkoyen/css-to-inline-styles": "^2.3",
        "vlucas/phpdotenv": "^5.6",
        "voku/portable-ascii": "^2.0",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
